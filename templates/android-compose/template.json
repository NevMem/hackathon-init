{
    "id": "android-compose",
    "marker": "android-compose",
    "depends_on": ["base", "shared-versions"],
    "defines": {
        "gradle_modules": [":android:app"]
    },
    "tests": [
        {
            "type": "gradle",
            "name": "build debug",
            "command": ":android:app:buildDebug"
        }
    ],
    "files": [
